#!/usr/bin/env php
<?php
declare(strict_types=1);

require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Nishadil\ImageTinify\Commands\TinifyCommand;

$application = new Application('ImageTinify CLI', '1.0.0');
$application->add(new TinifyCommand());
$application->setDefaultCommand('imagetinify', false);
$application->run();
